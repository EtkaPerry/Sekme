{
  "manifest_version": 3,
  "name": "Sekme",
  "version": "0.2",
  "description": "Custom new tab",
  "permissions": [
    "bookmarks",
    "storage",
    "activeTab",
    "history"
  ],
  "host_permissions": [
    "https://*/*"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "icons/16.png",
      "32": "icons/32.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    },
    "default_title": "Sekme Extension"
  },
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}
